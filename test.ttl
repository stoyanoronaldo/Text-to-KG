@prefix owl: <http://www.w3.org/2002/07/owl#>  .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:<http://www.w3.org/2001/XMLSchema#> .
@prefix fhir:<http://hl7.org/fhir/> .
@prefix example:<http://example.org/> .
@prefix icd10:<http://hl7.org/fhir/sid/icd-10-cm/> .

example:AlzheimersDisease
  a rdfs:Class ;
  rdfs:label "Alzheimer's disease" ;
  rdfs:comment "A degenerative brain disorder" ;
  fhir:code icd10:G30.9 .

example:DegenerativeBrainDisorder
  a rdfs:Class ;
  rdfs:label "Degenerative brain disorder" ;
  rdfs:subClassOf example:BrainDisorder .

example:BrainDisorder
  a rdfs:Class ;
  rdfs:label "Brain disorder" .

example:MediterraneanDiet
  a rdfs:Class ;
  rdfs:label "Mediterranean diet" ;
  rdfs:comment "A diet that may protect against Alzheimer's" .

example:MINDiet
  a rdfs:Class ;
  rdfs:label "MIND diet" ;
  rdfs:comment "A diet that may protect against Alzheimer's" .

example:InflammatorySaturatedFats
  a rdfs:Class ;
  rdfs:label "Inflammatory saturated fats" ;
  rdfs:comment "A type of fat that may contribute to Alzheimer's" .

example:Vitamins
  a rdfs:Class ;
  rdfs:label "Vitamins" ;
  rdfs:comment "Nutrients that may help protect against Alzheimer's" .

example:Minerals
  a rdfs:Class ;
  rdfs:label "Minerals" ;
  rdfs:comment "Nutrients that may help protect against Alzheimer's" .

example:Omega3s
  a rdfs:Class ;
  rdfs:label "Omega-3s" ;
  rdfs:comment "Nutrients that may help protect against Alzheimer's" .

example:Antioxidants
  a rdfs:Class ;
  rdfs:label "Antioxidants" ;
  rdfs:comment "Nutrients that may help protect against Alzheimer's" .

example:OxidativeStress
  a rdfs:Class ;
  rdfs:label "Oxidative stress" ;
  rdfs:comment "A condition associated with Alzheimer's" .

example:FreeRadicalDamage
  a rdfs:Class ;
  rdfs:label "Free radical damage" ;
  rdfs:comment "A type of damage associated with oxidative stress" .

example:AntioxidantIntake
  a rdfs:Class ;
  rdfs:label "Antioxidant intake" ;
  rdfs:comment "A potential protective factor against Alzheimer's" .

example:AlzheimersDisease a owl:Class ;
  rdfs:subClassOf example:DegenerativeBrainDisorder ;
  fhir:hasRiskFactor example:InflammatorySaturatedFats ;
  fhir:hasProtectiveFactor example:AntioxidantIntake ;
  fhir:hasAssociatedDisease example:Dementia .

example:Dementia
  a rdfs:Class ;
  rdfs:label "Dementia" ;
  rdfs:comment "A condition associated with Alzheimer's" .