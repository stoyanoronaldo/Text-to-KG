@prefix rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> .
@prefix sdo:<https://schema.org/> .
@prefix example:<http://example.org/> .

example:RealMadrid a sdo:SportsTeam ;
    rdfs:label "Real Madrid" .

example:Villarreal a sdo:SportsTeam ;
    rdfs:label "Villarreal" .

example:EstadioDeLaCeramica a sdo:Stadium ;
    rdfs:label "Estadio de la Ceramica" .

example:LaLiga a sdo:SportsLeague ;
    rdfs:label "LaLiga" .

example:ArdaGuler a sdo:Person ;
    rdfs:label "Arda Guler" ;
    sdo:memberOf example:RealMadrid .

example:Joselu a sdo:Person ;
    rdfs:label "Joselu" ;
    sdo:memberOf example:RealMadrid .

example:LucasVazquez a sdo:Person ;
    rdfs:label "Lucas Vazquez" ;
    sdo:memberOf example:RealMadrid .

example:Sorloth a sdo:Person ;
    rdfs:label "Sorloth" ;
    sdo:memberOf example:Villarreal .

example:Match a sdo:SportsEvent ;
    rdfs:label "Real Madrid vs Villarreal" ;
    sdo:location example:EstadioDeLaCeramica ;
    sdo:competitor example:RealMadrid ;
    sdo:competitor example:Villarreal ;
    sdo:startDate "2023-02-20"^^xsd:date ;
    sdo:endDate "2023-02-20"^^xsd:date .

example:FirstHalf a sdo:Event ;
    rdfs:label "First Half" ;
    sdo:partOf example:Match ;
    sdo:startDate "2023-02-20T19:00:00"^^xsd:dateTime ;
    sdo:endDate "2023-02-20T20:00:00"^^xsd:dateTime .

example:SecondHalf a sdo:Event ;
    rdfs:label "Second Half" ;
    sdo:partOf example:Match ;
    sdo:startDate "2023-02-20T20:00:00"^^xsd:dateTime ;
    sdo:endDate "2023-02-20T21:00:00"^^xsd:dateTime .

example:Goal1 a sdo:SportsEvent ;
    rdfs:label "Goal by Arda Guler" ;
    sdo:partOf example:FirstHalf ;
    sdo:agent example:ArdaGuler .

example:Goal2 a sdo:SportsEvent ;
    rdfs:label "Goal by Joselu" ;
    sdo:partOf example:FirstHalf ;
    sdo:agent example:Joselu .

example:Goal3 a sdo:SportsEvent ;
    rdfs:label "Goal by Lucas Vazquez" ;
    sdo:partOf example:FirstHalf ;
    sdo:agent example:LucasVazquez .

example:Goal4 a sdo:SportsEvent ;
    rdfs:label "Goal by Sorloth" ;
    sdo:partOf example:SecondHalf ;
    sdo:agent example:Sorloth .

example:Goal5 a sdo:SportsEvent ;
    rdfs:label "Goal by Sorloth" ;
    sdo:partOf example:SecondHalf ;
    sdo:agent example:Sorloth .

example:Goal6 a sdo:SportsEvent ;
    rdfs:label "Goal by Sorloth" ;
    sdo:partOf example:SecondHalf ;
    sdo:agent example:Sorloth .