@prefix rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema:<http://schema.org/> .
@prefix example:<http://example.org/> .

example:HarryPotter a schema:MovieSeries ;
    rdfs:label "Harry Potter" ;
    schema:creator example:JKRowling ;
    schema:productionCompany example:WarnerBrosPictures ;
    schema:hasPart example:HarryPotterAndThePhilosophersStone, 
                     example:HarryPotterAndTheDeathlyHallowsPart2, 
                     example:FantasticBeastsAndWhereToFindThem ;
    schema:genre schema:Fantasy .

example:JKRowling a schema:Person ;
    rdfs:label "J. K. Rowling" ;
    schema:nationality schema:British .

example:WarnerBrosPictures a schema:Organization ;
    rdfs:label "Warner Bros. Pictures" .

example:HarryPotterAndThePhilosophersStone a schema:Movie ;
    rdfs:label "Harry Potter and the Philosopher's Stone" ;
    schema:releaseDate "2001" .

example:HarryPotterAndTheDeathlyHallowsPart2 a schema:Movie ;
    rdfs:label "Harry Potter and the Deathly Hallows � Part 2" ;
    schema:releaseDate "2011" ;
    schema:boxOfficeGross 1000000000 .

example:FantasticBeastsAndWhereToFindThem a schema:Movie ;
    rdfs:label "Fantastic Beasts and Where to Find Them" ;
    schema:releaseDate "2016" .

example:DavidHeyman a schema:Person ;
    rdfs:label "David Heyman" ;
    schema:role schema:Producer ;
    schema:workedOn example:HarryPotter .

example:DanielRadcliffe a schema:Person ;
    rdfs:label "Daniel Radcliffe" ;
    schema:role schema:Actor ;
    schema:playedCharacter example:HarryPotterCharacter .

example:RupertGrint a schema:Person ;
    rdfs:label "Rupert Grint" ;
    schema:role schema:Actor ;
    schema:playedCharacter example:RonWeasleyCharacter .

example:EmmaWatson a schema:Person ;
    rdfs:label "Emma Watson" ;
    schema:role schema:Actor ;
    schema:playedCharacter example:HermioneGrangerCharacter .

example:ChrisColumbus a schema:Person ;
    rdfs:label "Chris Columbus" ;
    schema:role schema:Director ;
    schema:workedOn example:HarryPotter .

example:AlfonsoCuar�n a schema:Person ;
    rdfs:label "Alfonso Cuar�n" ;
    schema:role schema:Director ;
    schema:workedOn example:HarryPotter .

example:MikeNewell a schema:Person ;
    rdfs:label "Mike Newell" ;
    schema:role schema:Director ;
    schema:workedOn example:HarryPotter .

example:DavidYates a schema:Person ;
    rdfs:label "David Yates" ;
    schema:role schema:Director ;
    schema:workedOn example:HarryPotter .

example:MichaelGoldenberg a schema:Person ;
    rdfs:label "Michael Goldenberg" ;
    schema:role schema:Screenwriter ;
    schema:workedOn example:HarryPotterAndTheOrderOfThePhoenix .

example:SteveKloves a schema:Person ;
    rdfs:label "Steve Kloves" ;
    schema:role schema:Screenwriter ;
    schema:workedOn example:HarryPotter .

example:HarryPotterCharacter a schema:Character ;
    rdfs:label "Harry Potter" .

example:RonWeasleyCharacter a schema:Character ;
    rdfs:label "Ron Weasley" .

example:HermioneGrangerCharacter a schema:Character ;
    rdfs:label "Hermione Granger" .

example:LordVoldemort a schema:Character ;
    rdfs:label "Lord Voldemort" .