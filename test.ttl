@prefix rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema:<http://schema.org/> .
@prefix example:<http://example.org/> .

example:PremierLeague a schema:SportsOrganization ;
    rdfs:label "Premier League" ;
    schema:description "The highest level of the English football league system" ;
    schema:foundingDate "1992-02-20" ;
    schema:member example:Club1, example:Club2, ... (20 clubs) ;
    schema:leagueSystem example:EnglishFootballLeagueSystem .

example:EnglishFootballLeagueSystem a schema:SportsLeagueSystem ;
    rdfs:label "English Football League System" ;
    schema:hasPart example:PremierLeague, example:EFLChampionship .

example:EFLChampionship a schema:SportsLeague ;
    rdfs:label "EFL Championship" ;
    schema:partOf example:EnglishFootballLeagueSystem .

example:Club1 a schema:SportsTeam ;
    rdfs:label "Club 1" ;
    schema:memberOf example:PremierLeague .

example:Club2 a schema:SportsTeam ;
    rdfs:label "Club 2" ;
    schema:memberOf example:PremierLeague .

...

example:TVDeal a schema:MediaRights ;
    rdfs:label "TV Rights Deal" ;
    schema:value "£5 billion" ;
    schema:startDate "2022" ;
    schema:endDate "2025" ;
    schema:grantee example:Sky, example:BTGroup .

example:Sky a schema:Organization ;
    rdfs:label "Sky" ;
    schema:grantee example:TVDeal .

example:BTGroup a schema:Organization ;
    rdfs:label "BT Group" ;
    schema:grantee example:TVDeal .

example:CentralPayment a schema:MonetaryGrant ;
    rdfs:label "Central Payment" ;
    schema:value "£2.4 billion" ;
    schema:beneficiary example:PremierLeague .

example:SolidarityPayment a schema:MonetaryGrant ;
    rdfs:label "Solidarity Payment" ;
    schema:value "£343 million" ;
    schema:beneficiary example:EFLChampionship .