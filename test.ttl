@prefix rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema:<https://schema.org/> .
@prefix example:<http://example.org/> .

example:Influenza
  a schema:Disease ;
  rdfs:label "Influenza" ;
  schema:alternateName "the flu" ;
  schema:alternateName "flu" ;
  schema:description "an infectious disease caused by influenza viruses" .

example:InfluenzaVirus
  a schema:Virus ;
  rdfs:label "Influenza Virus" ;
  schema:hasPart example:InfluenzaAVirus, example:InfluenzaB Virus, example:InfluenzaCVirus, example:InfluenzaD Virus .

example:InfluenzaAVirus
  a schema:Virus ;
  rdfs:label "Influenza A Virus" ;
  schema:description "primarily found in aquatic birds, also widespread in various mammals, including humans and pigs" .

example:InfluenzaB Virus
  a schema:Virus ;
  rdfs:label "Influenza B Virus" ;
  schema:description "primarily infects humans" .

example:InfluenzaCVirus
  a schema:Virus ;
  rdfs:label "Influenza C Virus" ;
  schema:description "causes a mild infection, primarily in children" .

example:InfluenzaD Virus
  a schema:Virus ;
  rdfs:label "Influenza D Virus" ;
  schema:description "found in cattle and pigs, can infect humans but is not known to cause illness" .

example:Fever
  a schema:Symptom ;
  rdfs:label "Fever" ;
  schema:isPartOf example:Influenza .

example:RunnyNose
  a schema:Symptom ;
  rdfs:label "Runny Nose" ;
  schema:isPartOf example:Influenza .

example:SoreThroat
  a schema:Symptom ;
  rdfs:label "Sore Throat" ;
  schema:isPartOf example:Influenza .

example:MusclePain
  a schema:Symptom ;
  rdfs:label "Muscle Pain" ;
  schema:isPartOf example:Influenza .

example:Headache
  a schema:Symptom ;
  rdfs:label "Headache" ;
  schema:isPartOf example:Influenza .

example:Coughing
  a schema:Symptom ;
  rdfs:label "Coughing" ;
  schema:isPartOf example:Influenza .

example:Fatigue
  a schema:Symptom ;
  rdfs:label "Fatigue" ;
  schema:isPartOf example:Influenza .

example:Diarrhea
  a schema:Symptom ;
  rdfs:label "Diarrhea" ;
  schema:isPartOf example:Influenza .

example:Vomiting
  a schema:Symptom ;
  rdfs:label "Vomiting" ;
  schema:isPartOf example:Influenza .

example:Pneumonia
  a schema:Complication ;
  rdfs:label "Pneumonia" ;
  schema:isComplicationOf example:Influenza .

example:AcuteRespiratoryDistressSyndrome
  a schema:Complication ;
  rdfs:label "Acute Respiratory Distress Syndrome" ;
  schema:isComplicationOf example:Influenza .

example:Meningitis
  a schema:Complication ;
  rdfs:label "Meningitis" ;
  schema:isComplicationOf example:Influenza .

example:Encephalitis
  a schema:Complication ;
  rdfs:label "Encephalitis" ;
  schema:isComplicationOf example:Influenza .

example:Asthma
  a schema:PreExistingCondition ;
  rdfs:label "Asthma" ;
  schema:isAffectedBy example:Influenza .

example:CardiovascularDisease
  a schema:PreExistingCondition ;
  rdfs:label "Cardiovascular Disease" ;
  schema:isAffectedBy example:Influenza .